{
  "packageManager": "^pnpm@8.5.1",
  "name": "vite-react-template",
  "private": true,
  "version": "0.4.0",
  "scripts": {
    "production": "NODE_ENV=production yarn server",
    "server": "ts-node --project ./server/tsconfig.json -r tsconfig-paths/register server/src/index.ts",
    "tstest": "ts-node --project ./server/tsconfig.json -r tsconfig-paths/register server/src/test.ts",
    "preview": "vite preview",
    "dev": "concurrently -m 2 \"vite --host --port 3000\" \"nodemon\"",
    "build": "yarn build.lint && yarn build.vite",
    "build.lint": "concurrently -m 2 \"eslint src --fix\" \"eslint server/src --fix\"",
    "build.vite": "concurrently \"tsc --project ./server/tsconfig.json\" \"vite build\""
  },
