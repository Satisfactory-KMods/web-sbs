{
	"name": "sbs",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"build": "yarn build:prisma && yarn build:lint && yarn build:next",
		"prettier": "prettier --write .",
		"build:prisma": "prisma generate",
		"build:lint": "eslint src --fix && next lint --fix",
		"build:next": "next build",
		"dev": "yarn build:prisma && next dev",
		"devb": "yarn build:prisma && concurrently -m 2 \"next dev\" \"yarn startBackground\"",
		"startBackground": "ts-node --project ./tsconfig.json -r tsconfig-paths/register ./src/taskServer/index.ts",
		"start": "concurrently -m 2 \"next start\" \"yarn startBackground\""
	},
